@model Thing1.Models.AspNetUser

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>My Profile</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Email
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            Phone Number
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt>
            User Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt>
            Last Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt>
            First Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt>
            Preferred Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PreferredName)
        </dd>
        <dt>
            Program
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Program)
        </dd>
        <dt>
            Class Year
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ClassYear)
        </dd>
        <dt>
            T-Shirt Size
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TShirtSize)
        </dd>
    </dl>
</div>

<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id})
    <dd>[ @Html.ActionLink("Payment History", "Index", "PaymentHistory") ]</dd>
